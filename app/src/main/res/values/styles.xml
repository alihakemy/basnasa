<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="myMaterialButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:background">#E37494</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12dp</item>
        <item   name="android:fontFamily">@font/text_medium</item>

    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSizePercent" parent="">
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">66dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>


    <style name="ShapeAppearanceOverlay.App.CornerSizePercent2" parent="">
        <item name="cornerSizeTopRight">18dp</item>
        <item name="cornerSizeTopLeft">18dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>
    <style name="editext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@drawable/editetext_rec</item>
        <item name="android:ems">10</item>
        <item   name="android:fontFamily">@font/two_light</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/quantum_grey</item>
        <item name="colorControlActivated">#E37494</item>
    </style>

    <style name="BottomNavigationViewTextStyle">

        <item name="fontFamily">@font/text_medium</item>

    </style>


    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/two_light</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="Colors_Widget.MaterialComponents.Chip.Choice" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/white</item>
        <item name="cornerRadius">1dp</item>
        <item name="chipStrokeColor">#707070</item>
        <item name="chipStrokeWidth">.3dp</item>
        <item name="fontFamily">@font/two_light</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>